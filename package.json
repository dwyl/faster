{
  "name": "faster",
  "version": "2.3.8",
  "description": "faster way to reload your web project on all connected devices",
  "main": "index.js",
  "scripts": {
    "start": "node ./example/server.js",
    "faster": "bin/faster.js",
    "quick": "./node_modules/tape/bin/tape ./test/*.js",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/tape/bin/tape ./test/*.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/tape/bin/tape ./test/*.js && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "jshint": "./node_modules/jshint/bin/jshint -c .jshintrc --exclude-path .gitignore .",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=7d6a9a77660c9a52f34d752740c30a5e1617e672ff6fdb99c92a63b2f67787d0 ./node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info",
    "spec": "node ./node_modules/tape/bin/tape ./test/*.js | node_modules/tap-spec/bin/cmd.js"
  },
  "bin": {
    "faster": "./bin/faster.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ideaq/faster.git"
  },
  "keywords": [
    "faster",
    "reload",
    "and",
    "testing"
  ],
  "author": "@nelsonic <contact.nelsonic@gmail.com> (https://github.com/nelsonic)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ideaq/faster/issues"
  },
  "homepage": "https://github.com/ideaq/faster",
  "dependencies": {
    "chalk": "^1.0.0",
    "ignored": "^2.0.1",
    "listdirs": "^2.0.4",
    "ps-tree": "0.0.3",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "terminate": "^1.0.3"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "istanbul": "^0.3.8",
    "jshint": "^2.6.3",
    "mkdirp": "^0.5.0",
    "rimraf": "^2.3.2",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0"
  }
}
